{
	"name": "jweiland/powermail-salesforce",
	"type": "typo3-cms-extension",
	"description": "Powermail Salesforce integration",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"Powermail",
		"Salesforce",
		"Integration",
		"TYPO3",
		"TYPO3 CMS"
	],
	"homepage": "https://jweiland.net",
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "sfroemken@jweiland.net",
			"role": "Lead Developer"
		}
	],
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/powermail2salesforce/issues",
		"source": "https://github.com/jweiland-net/powermail2salesforce"
	},
	"require": {
		"typo3/cms-core": "^11.5.30",
		"in2code/powermail": "^10.7"
	},
	"require-dev": {
		"typo3/testing-framework": "^7.0.2",
		"phpunit/phpunit": "^9.6",
		"typo3/coding-standards": "^0.6",
		"friendsofphp/php-cs-fixer": "^3.14"
	},
	"replace": {
		"typo3-ter/powermail2salesforce": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\Powermail2salesforce\\": "Classes"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "powermail2salesforce",
			"app-dir": ".Build",
			"web-dir": ".Build/Web"
		}
	}
}
